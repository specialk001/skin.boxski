			</control>
			<control type="image">
				<left>50</left>
				<top>60</top>
				<width>760</width>
				<height>50</height>
				<aspectratio>stretch</aspectratio>
				<texture border="20">KeyboardEditArea.png</texture>
			</control>
			<control type="edit" id="312">
				<description>Edit Text</description>
				<left>55</left>
				<top>60</top>
				<width>750</width>
				<height>50</height>
				<font>font13</font>
